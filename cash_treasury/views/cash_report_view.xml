<odoo>
    <data>
        <record id="view_cash_treasury_report_line_tree" model="ir.ui.view">
            <field name="name">cash.treasury.report.line.tree</field>
            <field name="model">cash.treasury.report.line</field>
            <field name="arch" type="xml">
                <list string="Treasury Report" create="false" delete="false" edit="false">
                    <field name="date"/>
                    <field name="account_id"/>
                    <field name="journal_id"/>
                    <field name="name"/>
                    <field name="move_type"/>
                    <field name="debit"/>
                    <field name="credit"/>
                    <field name="balance_str" string="Balance After Transaction"/>
                </list>
            </field>
        </record>
    </data>
</odoo>