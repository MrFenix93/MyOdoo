<odoo>
    <data>

        <record id="module_category_cash_treasury" model="ir.module.category">
            <field name="name">Cash Treasury</field>
            <field name="sequence">10</field>
        </record>

        <!-- CASH OUT GROUPS -->
        <record id="group_cash_entry" model="res.groups">
            <field name="name">Cash Entry (Out)</field>
            <field name="category_id" ref="cash_treasury.module_category_cash_treasury"/>
        </record>

        <record id="group_cash_reviewer" model="res.groups">
            <field name="name">Cash Reviewer (Out)</field>
            <field name="category_id" ref="cash_treasury.module_category_cash_treasury"/>
        </record>

        <record id="group_cash_accountant" model="res.groups">
            <field name="name">Cash Accountant (Out)</field>
            <field name="category_id" ref="cash_treasury.module_category_cash_treasury"/>
        </record>

        <!-- CASH IN GROUPS -->
        <record id="group_cash_in_entry" model="res.groups">
            <field name="name">Cash Entry (In)</field>
            <field name="category_id" ref="cash_treasury.module_category_cash_treasury"/>
        </record>

        <record id="group_cash_in_accountant" model="res.groups">
            <field name="name">Cash Accountant (In)</field>
            <field name="category_id" ref="cash_treasury.module_category_cash_treasury"/>
        </record>

        <!-- ONE SHARED SUPER APPROVER -->
        <record id="group_cash_super_approver" model="res.groups">
            <field name="name">Cash Super Approver</field>
            <field name="category_id" ref="cash_treasury.module_category_cash_treasury"/>
        </record>

    </data>
</odoo>
